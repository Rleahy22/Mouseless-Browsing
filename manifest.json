{
    "name": "Mouseless Browsing",
    "version": "0.3.2",
    "description": "Makes browser navigation and interaction easier and possible without a mouse",
    "manifest_version": 2,
    "permissions": [
          "tabs", "http://*/*", "https://*/*"
    ],
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content.min.js"]
        }
    ]
}